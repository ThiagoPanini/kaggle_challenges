    "X_under, y_under = rus.fit_sample(X, y)\n",
    "df_undersampled = pd.DataFrame(X_under, columns=df.columns[:-1])\n",
    "df_undersampled['Class'] = y_under\n",
    "\n",
    "# Plotting results\n",
    "fig, axs = plt.subplots(nrows=1, ncols=2, figsize=(15, 6))\n",
