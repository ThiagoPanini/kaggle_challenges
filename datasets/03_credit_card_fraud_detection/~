    "test_under_pred = log_reg_under.predict(X_test_under)\n",
    "\n",
    "# Confusion matrix\n",
    "cv_cm = confusion_matrix(y_train, cv_predictions)\n",
    "test_cm = confusion_matrix(y_test, test_pred)\n",
    "cv_under_cm = confusion_matrix(y_train_under, cv_under_predictions)\n",
    "test_under_cm = confusion_matrix(y_test_under, test_under_pred)\n",
    "\n",
    "# Plotting\n",
    "fig = plt.figure(figsize=(10, 8))\n",
    "classes = ['Fraud', 'Non-Fraud']\n",
    "plt.subplot(221)\n",
    "plot_confusion_matrix(cv_cm, classes, title='Logistic Regression\\nCV Confusion Matrix')\n",
